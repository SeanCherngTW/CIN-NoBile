<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.linyuehlii.cinAPP" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.linyuehlii.cinAPP.StartActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.linyuehlii.cinAPP.MissionActivity"
            android:label="Mission"
            android:launchMode="singleTop"/>
        <activity android:name="com.example.linyuehlii.cinAPP.PointActivity"
            android:label="Point"
            android:launchMode="singleTop"/>
        <activity android:name="com.example.linyuehlii.cinAPP.StatActivity"
            android:label="Stat"
            android:launchMode="singleTop"/>
        <activity android:name="com.example.linyuehlii.cinAPP.SettingActivity"
            android:label="Setting"
            android:launchMode="singleTop"/>


    </application>


</manifest>